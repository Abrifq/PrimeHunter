{
    
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "template":"./node_modules/minami"
    },
    
    "plugins": ["plugins/markdown"],
    
    "recurseDepth": 10,
    
    "source": {
      
        "excludePattern": "",
       
        "includePattern": ".+\\.js(doc|x)?$",
      
        "type": "module"
    },
    
    "tags": {
        
        "allowUnknownTags": true,
        
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    
    "templates": {
       
        "cleverLinks": true,
       
        "monospaceLinks": true
    }
}