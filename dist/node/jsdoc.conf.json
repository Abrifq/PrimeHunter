{
    
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "template":"./node_modules/ink-docstrap/template/"
    },
    
    "plugins": ["plugins/markdown"],
    
    "recurseDepth": 10,
    
    "source": {
      
        "excludePattern": "",
       
        "includePattern": ".+\\.js(doc|x)?$",
      
        "type": "module"
    },
    
    "tags": {
        
        "allowUnknownTags": true,
        
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    
    "templates": {
       
        "cleverLinks": true,
        "monospaceLinks": true,
        "theme":"flatly",
        "systemName":"PrimeHunter",
        "footer":"Powered by ink-docStrap, jsdoc and love for programming!",
        "includeDate":true,
        "linenums":true,
        "navType":"vertical",
        "collapseSymbols":true,
        "inverseNav":true,
        "outputSourceFiles":true,
        "outputSourcePath":true,
        "syntaxTheme":"dark",
        "search":true,
        "sort":"longname",
        "wrap":true,
        "dateFormat":"D MMM YYYY"

    }
}